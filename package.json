    "multer": "^1.4.5-lts.1",
    "multer-s3": "^3.0.1",
    "request-ip": "^3.3.0",
    "sharp": "^0.32.1",
    "unzipper": "^0.10.11",
    "uuid": "^9.0.0"
  }
